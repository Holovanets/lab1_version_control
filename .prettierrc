{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/screens/(.*)$",
    "^@/components/(.*)$",
    "^@/actions/(.*)$",
    "^@/reducers/(.*)$",
    "^@/hooks/(.*)#",
    "^@/services/(.*)$",
    "^@/navigation/(.*)$",
    "^@/assets/(.*)$",
    "^@/providers/(.*)$",
    "^@/constants/(.*)$",
    "^@/regex/(.*)$",
    "^@/services/(.*)$",
    "^@/utils/(.*)$",
    "^@/types/(.*)$",
    "^../(.*)$",
    "^./(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
