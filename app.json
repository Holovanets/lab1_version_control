{
  "expo": {
    "name": "City Food",
    "slug": "CityFood",
    "version": "0.2.8",
    "orientation": "portrait",
    "icon": "./assets/fav.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash_pre_release.png",
      "resizeMode": "contain",
      "backgroundColor": "#C1272D"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#070707"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fav.png",
        "backgroundColor": "#070707"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.golovanets.restiki",
      "versionCode": 18,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD1sq7kt50Rx3K--skpSGRIxby0S6woHU8"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "cityfood.com.ua",
              "pathPrefix": "/(app)"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b9181333-1287-479c-83af-97ceae9be4a5"
      }
    }
  }
}
